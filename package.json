{
  "name": "thor-ui",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "lint:fix": "eslint './src/**/*.{js,jsx,tsx,ts,vue}' --fix",
    "lint": "eslint './src/**/*.{js,jsx,tsx,ts,vue}'",
    "prepare": "husky install",
    "dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "test": "vitest",
    "stub": "pnpm run -r --parallel stub",
    "build:lib": "pnpm run -C packages/build start",
    "docs:deploy": "bash deploy.sh",
    "postversion": "bash ./scripts/publish.sh && git push --follow-tags",
    "clean:dist": "rimraf dist",
    "clean": "pnpm run clean:dist",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@vue/shared": "^3.2.37",
    "@vueuse/core": "^9.1.0",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@esbuild-kit/cjs-loader": "^2.3.2",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@testing-library/vue": "^6.6.1",
    "@thor-ui/internal": "0.0.1",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "@vitejs/plugin-vue": "^3.0.0",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "@vitest/coverage-istanbul": "^0.26.3",
    "autoprefixer": "^10.4.7",
    "c8": "^7.12.0",
    "chalk": "^5.0.1",
    "consola": "^2.15.3",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.2.0",
    "fast-glob": "^3.2.11",
    "happy-dom": "^6.0.4",
    "husky": "^8.0.1",
    "istanbul": "^0.4.5",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup-plugin-esbuild": "^4.9.1",
    "sass": "^1.54.0",
    "tailwindcss": "^3.1.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "vitepress": "^1.0.0-alpha.4",
    "vitepress-theme-demoblock": "^1.4.2",
    "vitest": "^0.26.3",
    "vue-tsc": "^0.38.4"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue}": "eslint --cache --fix"
  }
}